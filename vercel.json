{
  "version": 2,
  "buildCommand": "echo 'No build needed for serverless functions'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/claim/(.*)",
      "destination": "/api/claim-view?id=$1"
    }
  ],
  "functions": {
    "api/generate.js": {
      "maxDuration": 30
    },
    "api/claim.js": {
      "maxDuration": 30
    }
  }
}